{
  "name": "gatsby-starter-ts",
  "description": "A TypeScript starter for Gatsby that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "John Korzhuk",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "preserve": "npm run build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "dotenv": "^8.2.0",
    "gatsby": "3.10.2",
    "gatsby-plugin-layout": "^2.11.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "immer-reducer": "^0.7.13",
    "lodash": "^4.17.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-open-weather-icons": "^0.1.1",
    "react-query": "^3.19.4",
    "recharts": "^2.1.0",
    "tsconfig-paths-webpack-plugin": "3.5.1"
  },
  "devDependencies": {
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@testing-library/dom": "7.31.2",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.1",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.9",
    "@types/react": "17.0.16",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "babel-jest": "27.0.6",
    "babel-plugin-root-import": "6.6.0",
    "babel-preset-gatsby": "1.10.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
